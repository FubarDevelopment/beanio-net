<?xml version='1.0' encoding='UTF-8' ?>
<beanio xmlns="http://www.beanio.org/2016/07"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.beanio.org/2016/07 http://www.beanio.org/2016/07/mapping.xsd">

  <stream name="wm1" format="csv" mode="write">
    <record name="record" class="BeanIO.Parser.WriteMode.IPerson, FubarDev.BeanIO.Test">
      <field name="firstName" getter="getFirstName" />
      <field name="lastName" />
      <field name="age" />
      <field name="birthDate" format="yyyy-MM-dd" />
    </record>  
  </stream>
  
</beanio>
